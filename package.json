  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Henrique1204/NodeJS-TS-API/issues"
  },
  "homepage": "https://github.com/Henrique1204/NodeJS-TS-API#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.4",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
